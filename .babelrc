{
   "presets": [
      ["env", {
         "modules": false,
         "targets": {
            "ie": "11"
         }
      }],
      "stage-2"
   ],
   "plugins": [
      "transform-runtime",
      "transform-decorators-legacy",
      "transform-class-properties",
      ["component", [
         {
            "libraryName": "element-ui",
            "styleLibraryName": "theme-default"
         }
      ]]
   ],
   "env": {
      "test": {
         "presets": ["env", "stage-2"],
         "plugins": ["istanbul"]
      }
   }
}

